<!DOCTYPE html>
<html>
<head>
  <title>仓库管理 - 后台</title>
  <%= csrf_meta_tags %>
  <%= csp_meta_tag %>

  <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
  <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
</head>

<body>
<% if @_current_admin %>
  <p>欢迎<%= @_current_admin.nickname %>
    <%= link_to '退出登录', center_sessions_path, method: :delete %></p>
  <p>
    <%= link_to '用户', center_users_path %>
  </p>
  <p>
    <%= link_to '物品', center_items_path %>
  </p>
  <p>
    <%= link_to '领用', center_acquires_path %>
  </p>
  <p>
    <%= link_to '日志', center_logs_path %>
  </p>
<% end %>
<%= yield %>
</body>
</html>
